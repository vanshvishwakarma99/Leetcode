class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
          
        unordered_map<int, int> Hi;
        int sum = 0;
        int count = 0;
        Hi[0]=1;

        for (int i=0; i<nums.size();i++) {
            sum += nums[i];
            if(Hi.find(sum-k)!=Hi.end()){
                count +=Hi[sum-k];
            }
            
            Hi[sum]++;
        }
        return count;
    }
};
